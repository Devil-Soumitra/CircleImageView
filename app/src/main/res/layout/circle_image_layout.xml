<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="25dp"
    android:layout_marginTop="10dp"
    android:layout_marginEnd="25dp"

    android:paddingBottom="10dp">

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/Profile_images25"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/lucifer_profile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </de.hdodenhof.circleimageview.CircleImageView>

    <LinearLayout
        android:id="@+id/circula_image_in_community"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:orientation="vertical"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/Profile_images25"
        app:layout_constraintStart_toStartOf="parent">
        <ImageView
            android:id="@+id/image_sector"
            android:layout_gravity="center"
            android:layout_width="340dp"
            android:layout_height="300dp"
            android:src="@drawable/lucifer_profile"
            android:contentDescription="@string/app_name"
            android:visibility="visible">
        </ImageView>
        <TextView
            android:padding="10dp"
            android:layout_marginTop="10dp"
            android:layout_width="340dp"
            android:layout_height="50dp"
            android:text="dtgthgtfhbgfbgfhythghtfrhgfttt">

        </TextView>
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>